import java.util.Scanner;
public class Fee
{
    public static void main(String args[])
    {Scanner sc=new Scanner(System.in);
    String ch;
    System.out.println("Enter the student type");
    ch=sc.nextLine();
    switch(ch)
    {
        case "MSDS":
        System.out.println("Enter tuition fee");
               float a=sc.nextFloat();
        System.out.println("Enter Bus fee");
               float b=sc.nextFloat();
               System.out.printf("The fees to be paid by the student is Rs.%.2f",(a+b));break;
         case "MSH":
        System.out.println("Enter tuition fee");
               a=sc.nextFloat();
        System.out.println("Enter Hostel fee");
               float c=sc.nextFloat();
               System.out.printf("The fees to be paid by the student is Rs.%.2f",(a+c));
               break;
         case "MGSDS":
        System.out.println("Enter tuition fee");
               a=sc.nextFloat();
        System.out.println("Enter Bus fee");
                b=sc.nextFloat();
               float d=(b/2+b);
               System.out.printf("The fees to be paid by the student is Rs.%.2f",(a+d));break;
       case "MGSH":
        System.out.println("Enter tuition fee");
                a=sc.nextFloat();
               float e=(a/2)+a;
        System.out.println("Enter Hostel fee");
                b=sc.nextFloat();
               System.out.printf("The fees to be paid by the student is Rs.%.2f",(e+b));break;
       
    }
}
}
